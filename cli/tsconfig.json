{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 20",
  "_version": "20.11.1",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "outDir": "./dist",
    "module": "Node16",
    "target": "es2022",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "allowJs": false
  },
}